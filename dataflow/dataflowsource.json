{
	"name": "dataflowsource",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedTextgit",
						"type": "DatasetReference"
					},
					"name": "sourcegit"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedTextgit",
						"type": "DatasetReference"
					},
					"name": "sinkgitplaque",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLakeStoragegit",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {Correlation id} as string,",
				"          {Operation name} as string,",
				"          Status as string,",
				"          {Event category} as string,",
				"          Level as string,",
				"          Time as string,",
				"          Subscription as string,",
				"          {Event initiated by} as string,",
				"          {Resource type} as string,",
				"          {Resource group} as string,",
				"          Resource as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcegit",
				"sourcegit sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Correlation id} as string,",
				"          {Operation name} as string,",
				"          Status as string,",
				"          {Event category} as string,",
				"          Level as string,",
				"          Time as string,",
				"          Subscription as string,",
				"          {Event initiated by} as string,",
				"          {Resource type} as string,",
				"          {Resource group} as string,",
				"          Resource as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'parquet') ~> sinkgitplaque"
			]
		}
	}
}